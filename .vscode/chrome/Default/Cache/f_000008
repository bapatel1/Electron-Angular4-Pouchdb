/***/ "../../../../../src/app/_pouchdb/pouchdb-service/pouchdb-adapter.ts":
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
/* harmony import */ var __WEBPACK_IMPORTED_MODULE_0_pouchdb__ = __webpack_require__("../../../../pouchdb/lib/index-browser.es.js");
/* harmony import */ var __WEBPACK_IMPORTED_MODULE_1_rxjs_BehaviorSubject__ = __webpack_require__("../../../../rxjs/BehaviorSubject.js");
/* harmony import */ var __WEBPACK_IMPORTED_MODULE_1_rxjs_BehaviorSubject___default = __webpack_require__.n(__WEBPACK_IMPORTED_MODULE_1_rxjs_BehaviorSubject__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "a", function() { return PouchDbAdapter; });


var PouchDbAdapter = (function () {
    function PouchDbAdapter(remoteCouchDBAddress, debugMode) {
        var _this = this;
        // rxjs behaviour subjects to expose stats flags
        this.syncStatus = new __WEBPACK_IMPORTED_MODULE_1_rxjs_BehaviorSubject__["BehaviorSubject"](false);
        this.couchDbUp = new __WEBPACK_IMPORTED_MODULE_1_rxjs_BehaviorSubject__["BehaviorSubject"](false);
        this._remoteCouchDBAddress = remoteCouchDBAddress;
        // string function to extract the database name from the URL
        this._pouchDbName = remoteCouchDBAddress
            .substr(remoteCouchDBAddress.lastIndexOf('/') + 1);
        // init local PouchDB
        this._pouchDB = new __WEBPACK_IMPORTED_MODULE_0_pouchdb__["a" /* default */](this._pouchDbName);
        if (debugMode) {
            __WEBPACK_IMPORTED_MODULE_0_pouchdb__["a" /* default */].debug.enable('*');
        }
        console.log('Connecting local db:', this._pouchDB);
        // init PouchDB adapter for remote CouchDB
        this._couchDB = new __WEBPACK_IMPORTED_MODULE_0_pouchdb__["a" /* default */](remoteCouchDBAddress);
        console.log('Connecting remote db:', this._couchDB);
        // sync the PouchDB and CouchDB
        this._pouchDB.sync(this._couchDB, {
            live: true,
            retry: true,
            continuous: true
        })
            .on('paused', function (err) { _this.syncStatusUpdate(); })
            .on('change', function (info) { _this.syncStatusUpdate(); });
    }
    // pretty basic and crude function
    // return a Promise with the first 20 docs from allDocs as is
    PouchDbAdapter.prototype.getDocs = function (howmany) {
        var _this = this;
        return new Promise(function (resolve) {
            _this._pouchDB.allDocs({
                include_docs: true,
                limit: howmany
            })
                .then(function (result) {
                resolve(result);
            })
                .catch(function (error) {
                console.log(error);
            });
        });
    };
    PouchDbAdapter.prototype.post = function (doc) {
        var _this = this;
        return new Promise(function (resolve) {
            _this._pouchDB.post(doc)
                .then((function (response) {
                resolve(response);
            }))
                .catch(function (error) {
                console.log(error);
            });
        });
    };
    // function to call the below functions
    // then update the rxjs BehaviourSubjects with the 
    // results
    PouchDbAdapter.prototype.syncStatusUpdate = function () {
        var _this = this;
        this.checkPouchCouchSync()
            .then(function (result) {
            _this.syncStatus.next(result);
        });
        this.checkCouchUp()
            .then(function (result) {
            _this.couchDbUp.next(result);
        });
    };
    // part of the JSON returned by PouchDB from the info() method
    // is "update_seq". When these numbers are equal then the databases
    // are in sync. The way its buried in the JSON means some string
    // functions are required to extract it
    PouchDbAdapter.prototype.checkPouchCouchSync = function () {
        // if both objects exist then make a Promise from both their
        // info() methods
        if (this._pouchDB && this._couchDB) {
            return Promise.all([this._pouchDB.info(), this._couchDB.info()])
                .then(function (results) {
                return (Number(String(results[0]
                    .update_seq)
                    .split('-')[0])
                    ===
                        Number(String(results[1]
                            .update_seq)
                            .split('-')[0]));
            })
                .catch(function (error) { return false; });
        }
        else {
            // if one of the PouchDB or CouchDB objects doesn't exist yet
            // return resolve false
            return Promise.resolve(false);
        }
    };
    // fairly self explanatory function to make a 
    // GET http request to the URL and return false
    // if an error status or a timeout occurs, true if 
    // successful.
    PouchDbAdapter.prototype.checkCouchUp = function () {
        var _this = this;
        return new Promise(function (resolve, reject) {
            var xhr = new XMLHttpRequest();
            xhr.open('GET', _this._remoteCouchDBAddress, true);
            xhr.onload = function () {
                if (xhr.status >= 200 && xhr.status < 300) {
                    resolve(true);
                }
                else {
                    resolve(false);
                }
            };
            xhr.onerror = function () {
                resolve(false);
            };
            xhr.send();
        });
    };
    return PouchDbAdapter;
}());

//# sourceMappingURL=pouchdb-adapter.js.map

/***/ }),

